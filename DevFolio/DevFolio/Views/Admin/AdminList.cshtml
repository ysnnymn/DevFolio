@model List<DevFolio.Models.TblAdmin>
@{
    ViewBag.Title = "AdminList";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<h2>Admin Listesi</h2>
<table class="table table-bordered">
   <tr>
       <th>#</th>
       <th>Kullanıcı Adı</th>
       <th>Sil</th>
       <th>Güncelle</th>

   </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AdminID</td>
            <td>@item.Username</td>
            <td><a href="/Admin/DeleteAdmin/@item.AdminID" class="btn btn-danger">Sil</a></td>
            <td><a href="/Admin/UpdateAdmin/@item.AdminID" class="btn btn-success">Güncelle</a></td>
        </tr>
    }
</table> 
<a href="/Admin/CreateAdmin/" class="btn btn-info" type="text/asa">Yeni Admin Girişi</a>
